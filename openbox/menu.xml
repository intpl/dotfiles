<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="term">
			<action name="Execute">
				<execute>terminology -2</execute>
			</action>
		</item>
		<item label="evince">
			<action name="Execute">
				<execute>evince</execute>
			</action>
		</item>
		<item label="file">
			<action name="Execute">
				<execute>thunar</execute>
			</action>
		</item>
		<item label="www">
			<action name="Execute">
				<execute>chromium-browser</execute>
			</action>
		</item>
		<item label="skype">
			<action name="Execute">
				<execute>skype</execute>
			</action>
		</item>
		<separator/>
		<item label="-&gt; pdfs/">
			<action name="Execute">
				<execute>thunar ~/Dropbox_encrypted/pdfs</execute>
			</action>
		</item>
		<separator/>
		<item label="torrent">
			<action name="Execute">
				<execute>transmission-gtk</execute>
			</action>
		</item>
		<item label="hexchat">
			<action name="Execute">
				<execute>hexchat</execute>
			</action>
		</item>
		<item label="baobab">
			<action name="Execute">
				<execute>baobab</execute>
			</action>
		</item>
		<separator/>
		<menu id="root-menu-74225" label="openbox">
			<item label="arandr">
				<action name="Execute">
					<execute>arandr</execute>
				</action>
			</item>
			<separator/>
			<item label="obconf">
				<action name="Execute">
					<execute>obconf</execute>
				</action>
			</item>
			<item label="obmenu">
				<action name="Execute">
					<execute>obmenu</execute>
				</action>
			</item>
			<separator/>
			<item label="reconf">
				<action name="Reconfigure"/>
			</item>
			<item label="restart">
				<action name="Restart"/>
			</item>
			<item label="exit">
				<action name="Execute">
					<execute>openbox --exit</execute>
				</action>
			</item>
		</menu>
		<menu id="root-menu-03421" label="systemctl">
			<item label="nm restart">
				<action name="Execute">
					<execute>sudo service network-manager restart</execute>
				</action>
			</item>
			<separator/>
			<item label="suspend">
				<action name="Execute">
					<execute>sudo systemctl suspend</execute>
				</action>
			</item>
			<item label="hibernate">
				<action name="Execute">
					<execute>sudo systemctl hibernate</execute>
				</action>
			</item>
			<item label="reboot">
				<action name="Execute">
					<execute>sudo systemctl reboot</execute>
				</action>
			</item>
			<item label="poweroff">
				<action name="Execute">
					<execute>sudo systemctl poweroff</execute>
				</action>
			</item>
		</menu>
	</menu>
</openbox_menu>
